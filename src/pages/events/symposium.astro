</Layout>

<style>
	em {
		font-weight: bold;
	}
	table {
		/* width: 100%; */
		border-collapse: collapse;
		overflow-x: scroll;
	}
	thead {
		border-bottom: var(--theme-spacing-base) solid
			var(--background-colour-darker);
	}
	td {
		padding-bottom: calc(2 * var(--theme-spacing-base));
		padding-left: 0;
		padding-right: 0;
		vertical-align: middle;
	}
	tr {
		transition: background-color var(--transition-speed-medium);
	}
	tr:hover {
		background-color: var(--background-colour-dark);
	}
	td {
		padding-left: calc(2 * var(--theme-spacing-base));
		padding-right: calc(2 * var(--theme-spacing-base));
	}

	nav#internal-nav {
		display: flex;
		justify-content: center;
		align-items: center;
		flex-overflow: auto;
	}
	nav#internal-nav ul {
		display: flex;
		list-style-type: none;
		margin: 0;
		padding: 0;
	}
	nav#internal-nav ul li {
		margin: 0.5rem;
	}
	nav#internal-nav ul li a {
		color: var(--theme-colour);
		text-decoration: none;
		font-weight: bold;
	}
	/* nav#internal-nav ul li a:hover { */
	/* } */
	nav#internal-nav ul li a span {
		display: inline-block;
		padding: 0.5rem;
		border-radius: var(--border-radius-medium);
		transition: background-color var(--transition-speed-medium);
	}
	nav#internal-nav ul li a span:hover {
		background-color: var(--background-colour-dark);
	}
	p.subtitle {
		margin: 0;
		padding: 0;
		font-weight: bold;
	}
	h2 {
		margin-bottom: 0.5rem;
	}
</style>


